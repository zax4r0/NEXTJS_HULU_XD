{
  "name": "spamflix",
  "private": true,
  "version": "0.0.1",
  "description": "hulu clone using nextjs",
  "keywords": [
    "#Nextjs #prisma #mongodb #movie #music #fun "
  ],
  "license": "MIT",
  "author": "zax4r0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate:prisma": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "typecheck": "tsc",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore . && prettier --write ./**/*.{ts,tsx}",
    "prettier": "prettier --write ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@popperjs/core": "^2.10.2",
    "@prisma/client": "3.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "apollo-server-micro": "3.4.0",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "formik": "^2.2.9",
    "graphql": "15.6.1",
    "graphql-scalars": "1.12.0",
    "graphql-tag": "2.12.5",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "12.0.2",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.4.0",
    "next-with-apollo": "^5.2.1",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "7.0.1",
    "react-player": "^2.9.0",
    "sharp": "^0.30.5",
    "tailwind-scrollbar-hide": "^1.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "^2.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-tailwindcss": "^1.14.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prisma": "3.3.0",
    "tailwindcss": "^2.2.7",
    "ts-node": "10.3.0",
    "typescript": "4.4.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
